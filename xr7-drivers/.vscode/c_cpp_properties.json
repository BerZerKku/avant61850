{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../linux-raspbian-3.18.11/include",
                "${workspaceFolder}/../linux-raspbian-3.18.11/include/uapi",
                "${workspaceFolder}/../linux-raspbian-3.18.11/arch/arm/include",
                "${workspaceFolder}/../linux-raspbian-3.18.11/arch/arm/include/generated/",
                "${workspaceFolder}/../linux-raspbian-3.18.11/arch/arm/mach-bcm2708/include/",
                "${workspaceFolder}/../tools/gcc-linaro-arm-linux-gnueabihf-raspbian-x64/arm-linux-gnueabihf/libc/usr/include/"
            ],
            "defines": [
                "__KERNEL__"
            ],
            "compilerPath": "${workspaceFolder}/../tools/gcc-linaro-arm-linux-gnueabihf-raspbian-x64/bin/arm-linux-gnueabihf-gcc",
            "cStandard": "c99",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "browse": {
                "path": [
                    "${workspaceRoot}/**"
                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "compilerArgs": [
                "-Wall"
            ]
        }
    ],
    "version": 4
}